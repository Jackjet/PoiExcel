<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">

	<context-param>
		<param-name>extension</param-name>
		<param-value>xls</param-value>
	</context-param>


	<servlet>
		<servlet-name>ExcelWriteServlet2</servlet-name>
		<servlet-class>com.excel.test.ExcelWriteServlet2</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ExcelWriteServlet2</servlet-name>
		<url-pattern>/servlet/ExcelWriteServlet2</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>ExcelWriteServlet3</servlet-name>
		<servlet-class>com.excel.test.ExcelWriteServlet3</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ExcelWriteServlet3</servlet-name>
		<url-pattern>/servlet/ExcelWriteServlet3</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>PoiUtilsTest</servlet-name>
		<servlet-class>com.excel.test.PoiUtilsTest</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PoiUtilsTest</servlet-name>
		<url-pattern>/servlet/PoiUtilsTest</url-pattern>
	</servlet-mapping>
</web-app>
